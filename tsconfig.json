{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
        "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "module": "Node16",
    "moduleResolution": "nodenext",
    "target": "ESNext",
    "types": ["node", "debug"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/lib":["src/lib/*"]
    }
  }
}
